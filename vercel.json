{
  "version": 2,
  "installCommand": "npm install typescript",
  "devCommand": "npm run postinstall",
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./src/(.*)",
      "dest": "./src/server/routes/index.js"
    }
  ]
}
